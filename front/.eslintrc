{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": { "jsx": true }
  },
  "rules": {
    "indent": [ "error", 2 ],
    "no-undef": 0,
    "camelcase": 0,
    "react/prop-types": [ 2 ],
    "react/react-in-jsx-scope": 0
  },
  "plugins": [ "react" ]
}
